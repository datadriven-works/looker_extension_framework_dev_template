{
  "name": "looker-extension-starter",
  "version": "0.1.0",
  "private": false,
  "description": "Starter template for a Looker Extension Framework application",

  "scripts": {
    "clean": "rimraf dist",
    "start": "webpack serve --config webpack.develop.js",
    "build": "cross-env BABEL_ENV=build webpack --config webpack.prod.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "jest"
  },

  "engines": { "node": ">=18" },

  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",

    "@looker/extension-sdk": "^24.20.0",
    "@looker/extension-sdk-react": "^24.20.0",
    "@looker/sdk": "^24.20.0",

    "@mui/material": "^5.15.15",
    "@mui/icons-material": "^5.15.15",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.1"
  },

  "devDependencies": {
    "typescript": "^5.7.2",
    "@types/react": "^17.0.62",
    "@types/react-dom": "^17.0.19",
    "@types/node": "^22.10.1",
    "@types/webpack-env": "^1.18.8",

    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "babel-loader": "^9.2.1",

    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",

    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5",

    "eslint": "^9.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",

    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "@types/jest": "^29.5.14",
    "jest-environment-jsdom": "^29.7.0"
  },

  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  }
}
